{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@store/*": [
        "./src/store/*"
      ]
    }
  }
}